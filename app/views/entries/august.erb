
<div class="months">
<b>August</b><br><br/>
<% @entries.each do |entry| %>

  <% if entry.month=="august" %>
    <li>
        <a class="monthlist" href="/entries/<%= entry.id %>" >
         Subject: <%= entry.subject %> 
    </a> </li><br>
    
  <% end %>
<% end %>
</ul>
<h3>Total <i>$</i><%= @august_total%></h3>

</div>