
<div class="months">
<b>June</b><br><br/>
<% @entries.each do |entry| %>
 
  <% if entry.month=="june" %>
    <li>
        <a class="monthlist" href="/entries/<%= entry.id %>" >
         Subject: <%= entry.subject %> 
    </a> </li><br>
    
  <% end %>
<% end %>
</ul>
<h3 class>Total <i>$</i><%= @june_total%></h3>

</div>